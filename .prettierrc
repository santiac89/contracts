{
  "semi": false,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "printWidth": 120,
  "singleQuote": true,
  "useTabs": false,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "trailingComma": "none",
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "singleQuote": false,
        "explicitTypes": "always"
      }
    }
  ]
}
